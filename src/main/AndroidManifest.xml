<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="example.com.asusshoping" >

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AsusShoping"
        tools:targetApi="31" >
        <activity
            android:name=".Soybean_Paste_Stew"
            android:exported="false"
            android:label="@string/title_activity_soybean_paste_stew"
            android:theme="@style/Theme.AsusShoping.NoActionBar" />
        <activity
            android:name=".Curry"
            android:exported="false"
            android:label="@string/title_activity_curry"
            android:theme="@style/Theme.AsusShoping.NoActionBar" />
        <activity
            android:name=".stir_fried_pork"
            android:exported="false"
            android:label="@string/title_activity_stir_fried_pork"
            android:theme="@style/Theme.AsusShoping.NoActionBar" />
        <activity
            android:name=".Kimchi_stew"
            android:exported="false"
            android:label="@string/title_activity_kimchi_stew"
            android:theme="@style/Theme.AsusShoping.NoActionBar" />
        <activity
            android:name=".Map"
            android:exported="false" />
        <activity
            android:name=".Search"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>